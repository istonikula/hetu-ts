{
  "name": "hetu-ts",
  "version": "0.0.1",
  "description": "Finnish social security number (hetu) TypeScript library",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "lint": "tslint src/**/*.ts test/**/*.ts",
    "jest": "jest",
    "test": "yarn run lint && yarn run jest",
    "clean": "rm -rf lib/*",
    "build": "yarn run clean && tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/istonikula/hetu-ts.git"
  },
  "author": "Isto Nikula <isto.nikula@iki.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/istonikula/hetu-ts/issues"
  },
  "homepage": "https://github.com/istonikula/hetu-ts",
  "dependencies": {
    "js-joda": "^1.10.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^2.8.1"
  },
  "tags": [
    "typescript",
    "hetu",
    "ssn"
  ],
  "keywords": [
    "typescript",
    "hetu",
    "ssn"
  ],
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
